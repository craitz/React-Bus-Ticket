{
    "name": "react-bus-ticket",
    "version": "0.1.0",
    "private": true,
    "reactToolbox": {
        "include": [
            "BUTTON",
            "DATE_PICKER",
            "PROGRESS_BAR",
            "INPUT",
            "SNACKBAR",
            "TABS",
            "MENU",
            "DROPDOWN"
        ],
        "customProperties": {
            "color-primary": "#424242",
            "color-accent": "#F44336",
            "input-text-highlight-color": "#F0AD4E",
            "animation-duration": "0.3s",
            "snackbar-background-color": "rgba(0,0,0,.7)",
            "snackbar-border-radius": "4px"
        },
        "output": "src/styles/react-toolbox"
    },
    "dependencies": {
        "animate.css": "^3.5.2",
        "bootstrap": "^3.3.7",
        "firebase": "^4.1.3",
        "material-design-icons": "^3.0.1",
        "moment": "^2.18.1",
        "muicss": "^0.9.20",
        "prop-types": "^15.5.10",
        "react": "^15.6.1",
        "react-bootstrap": "^0.31.0",
        "react-bootstrap-maskedinput": "^0.2.1",
        "react-collapsible": "^1.4.0",
        "react-datetime": "^2.8.10",
        "react-dom": "^15.6.1",
        "react-fontawesome": "^1.6.1",
        "react-ink": "^6.2.0",
        "react-loading-animation": "^1.3.0",
        "react-redux": "^5.0.5",
        "react-responsive-accordion": "^1.0.0",
        "react-router-dom": "^4.1.1",
        "react-scripts": "1.0.10",
        "react-select": "^1.0.0-rc.5",
        "react-spinner-children": "^1.0.7",
        "react-toastr": "^2.8.2",
        "redux": "^3.7.1",
        "redux-logger": "^3.0.6",
        "redux-promise-middleware": "^4.3.0",
        "redux-thunk": "^2.2.0",
        "seamless-immutable": "^7.1.2",
        "seamless-immutable-mergers": "^7.0.0",
        "vanilla-masker": "^1.2.0"
    },
    "devDependencies": {
        "enzyme": "^2.9.1",
        "node-sass-chokidar": "0.0.3",
        "npm-run-all": "^4.0.2",
        "react-scripts": "1.0.10",
        "react-test-renderer": "^15.6.1",
        "react-toolbox": "^2.0.0-beta.6",
        "react-toolbox-themr": "^1.0.2",
        "redux-mock-store": "^1.2.3"
    },
    "scripts": {
        "build-css": "node-sass-chokidar src/styles/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar src/styles/ -o src/ --watch --recursive",
        "start-js": "react-scripts start",
        "start": "npm-run-all -p watch-css start-js",
        "build": "npm run build-css && react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "toolbox": "react-toolbox-themr"
    }
}